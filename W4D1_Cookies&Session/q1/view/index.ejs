<form name='AddCookie' method='POST'>
    <label>Key <input name="key" /></label>
    <label>Value <input name="value"/></label>
    <input type="submit" />
</form>
<table>
    <th>
        Key
    </th>
    <th>
        Value
    </th>
    <% for(item in cookies){%> <tr><td><%=item%></td><td><%=cookies[item]%></td></tr><%} %>
</table>